<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="5dp"
        android:right="5dp"
        >
        <rotate
            android:fromDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="135">
            <shape android:shape="line">
                <stroke android:width="1.5dp" android:color="#777777" />
                <corners android:radius="15dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:left="5dp"
        android:right="5dp"
        >
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke android:width="1.5dp" android:color="#777777" />
                <corners android:radius="15dp"/>
            </shape>
        </rotate>
    </item>

  <!--  <item>
        <shape android:shape="oval">
                <stroke android:width="0.09dp"
                    android:color="#929292" />
        </shape>

    </item>-->


</layer-list>